<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import com.jfoenix.controls.JFXComboBox?>
<?import com.jfoenix.controls.JFXPasswordField?>
<?import com.jfoenix.controls.JFXTextField?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="signUpPanel" maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity" prefHeight="367.0" prefWidth="796.0"
            style="-fx-background-color: white; -fx-border-color: #FF962F; -fx-border-width: 0.25em;"
            xmlns="http://javafx.com/javafx/8.0.141"
            fx:controller="lk.ijse.roombooking.controller.usercontroller.SignUpController">
    <children>
        <JFXTextField fx:id="userNameText" focusColor="#ff962f" labelFloat="true" layoutX="360.0" layoutY="61.0"
                      onAction="#userNameAction" prefHeight="39.0" prefWidth="389.0" promptText="User Name">
            <font>
                <Font name="Arial Black" size="18.0"/>
            </font>
        </JFXTextField>
        <JFXPasswordField fx:id="passwordText" focusColor="#ff962f" labelFloat="true" layoutX="360.0" layoutY="135.0"
                          onAction="#passwordAction" prefHeight="39.0" prefWidth="389.0" promptText="Password">
            <font>
                <Font name="Arial Black" size="18.0"/>
            </font>
        </JFXPasswordField>
        <JFXButton fx:id="cancelButton" layoutX="583.0" layoutY="286.0" onAction="#cancelButtonAction" prefHeight="36.0"
                   prefWidth="160.0" style="-fx-background-color: #FF962F; -fx-background-radius: 3em;" text="Cancel"
                   textFill="WHITE"/>
        <Label layoutX="684.0" prefHeight="46.0" prefWidth="107.0" text="Sign Up" textAlignment="RIGHT"
               textFill="#ff962f">
            <font>
                <Font name="System Bold" size="29.0"/>
            </font>
        </Label>
        <JFXPasswordField fx:id="confirmPasswordText" disable="true" focusColor="#ff962f" labelFloat="true"
                          layoutX="360.0" layoutY="207.0" onAction="#confirmPasswordAction" prefHeight="39.0"
                          prefWidth="389.0" promptText="Confirm Password">
            <font>
                <Font name="Arial Black" size="18.0"/>
            </font>
        </JFXPasswordField>
        <JFXButton fx:id="logInButton" layoutX="54.0" layoutY="286.0" onAction="#logInButtonAction" prefHeight="36.0"
                   prefWidth="120.0" style="-fx-background-color: #FF962F; -fx-background-radius: 3em;" text="Log In"
                   textFill="WHITE"/>
        <ImageView fitHeight="55.0" fitWidth="58.0" layoutX="257.0" layoutY="53.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../../assets/Circled%20User%20Male_64px.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="55.0" fitWidth="58.0" layoutX="257.0" layoutY="127.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../../assets/Key_64px.png"/>
            </image>
        </ImageView>
        <ImageView fitHeight="55.0" fitWidth="58.0" layoutX="257.0" layoutY="199.0" pickOnBounds="true"
                   preserveRatio="true">
            <image>
                <Image url="@../../assets/Key_64px.png"/>
            </image>
        </ImageView>
        <JFXComboBox fx:id="userTypeCombo" focusColor="#ff962f" layoutX="26.0" layoutY="139.0" prefHeight="31.0"
                     prefWidth="197.0"/>
        <Label layoutX="66.0" layoutY="108.0" prefHeight="31.0" prefWidth="120.0" text="User Type" textFill="#4d4d4d">
            <font>
                <Font name="Arial Black" size="18.0"/>
            </font>
        </Label>
    </children>
</AnchorPane>
